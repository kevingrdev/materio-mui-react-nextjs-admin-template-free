{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/401",
        "destination": "/401/index.html"
      },
      {
        "source": "/404",
        "destination": "/404/index.html"
      },
      {
        "source": "/500",
        "destination": "/500/index.html"
      },
      {
        "source": "/account-settings",
        "destination": "/account-settings/index.html"
      },
      {
        "source": "/conductor/:userId",
        "destination": "/conductor/[id]/index.html"
      },
      {
        "source": "/conductores",
        "destination": "/conductores/index.html"
      },
      {
        "source": "/pages/login",
        "destination": "/pages/login/index.html"
      },
      {
        "source": "/pages/error",
        "destination": "/pages/error/index.html"
      },
      {
        "source": "/pages/register",
        "destination": "/pages/register/index.html"
      },
      {
        "source": "/user/:userId",
        "destination": "/user/[id]/index.html"
      },
      {
        "source": "/404",
        "destination": "/404.html"
      }
    ]
  }
}